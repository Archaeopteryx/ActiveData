{
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		},
		"pyLibrary.env.big_data.MAX_STRING_SIZE": 100000000,
		"jx_elasticsearch.meta.ENABLE_META_SCAN": true,
		"jx_base.expressions.ALLOW_SCRIPTING": true
	},
	"request_logs": {
		"host": "http://localhost",
		"port": 9200,
		"index": "activedata_requests",
		"type": "request_log",
		"tjson": false,
		"schema": {
			"$ref": "//../schema/request_log.schema.json"
		}
	},
	"saved_queries":{
		"host": "http://localhost",
		"port": 9200,
		"index": "saved_queries",
		"type": "query",
		"debug": true
	},
	"use": "elasticsearch",
	"elasticsearch": {
		"host": "http://localhost",
		"port": 9200,
		"index": "testdata",
		"type": "test_result",
		"debug": false
	},
	"debug": {
		"trace": true,
		"cprofile": false,
		"log": [
			{
				"log_type": "console"
			},
			{
				"log_type": "ses",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][STAGING] Problem in ActiveData Frontend"
			}
		]
	}

}
