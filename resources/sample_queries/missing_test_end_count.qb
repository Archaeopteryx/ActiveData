{
	"from":{
		"type":"elasticsearch",
		"settings":{
			"host":"http://52.10.116.229",
			"index":"unittest",
			"type":"test_results"
		}
	},
	"select":{"aggregate":"count"},
	"edges":[{"name":"test","value":["run.suite","result.test"]}],
	"where":{"and":[
		{"exists":"result.missing_test_end"},
		{"gte":{"run.stats.start_time":"{{today|week}}"}}
	]}
}
