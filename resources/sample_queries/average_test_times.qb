{
	"from":{
		"type":"elasticsearch",
		"settings":{
			"index":"unittest",
			"type":"test_result",
			"host":"http://52.10.189.133"
		}
	},
	"select":[
		{
			"name":"total_duration",
			"value":"run.stats.duration",
			"aggregate":"sum"
		},
		{
			"name":"count",
			"value":"run.stats.duration",
			"aggregate":"count"
		},
		{
			"name":"avg_duration",
			"value":"run.stats.duration",
			"aggregate":"average"
		}
	],
	"groupby":["machine.platform","machine.os","run.suite","result.test"],
	"where":{"and":[
		{"gte":{"run.stats.start_time":"{{today|week-week}}"}},
		{"lt":{"run.stats.start_time":"{{today|week}}"}}
	]},
	"limit":100000
}
