{
	"from":{
		"type":"elasticsearch",
		"settings":{
			"host":"http://54.149.35.214",
			"index":"unittest",
			"type":"test_result"
		}
	},
	"select":[{"aggregate":"count"}],
	"edges":["build.branch","result.test"],
	"where":{"and":[
		{"eq":{"result.ok":false}},
		{"gt":{"run.timestamp":"{{today|week}}"}}
	]},
	"format":"table",
	"limit":100000
}
