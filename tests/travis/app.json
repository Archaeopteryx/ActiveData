{
	"is_travis": true,
	"use": "elasticsearch",
	"service_url": "http://127.0.0.1:5000/query",
	"sql_url": "http://127.0.0.1:5000/sql",
	"flask": {
		"host": "0.0.0.0",
		"port": 5000,
		"debug": false,
		"threaded": true,
		"processes": 1,
		"ssl_context": "adhoc",
		"allow_exit": true
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		},
 		"pyLibrary.env.big_data.MAX_STRING_SIZE": 100000000,
		"jx_elasticsearch.meta.ENABLE_META_SCAN": true,
		"jx_base.expressions.ALLOW_SCRIPTING": true
	},
	"request_logs": {
		"host": "http://localhost",
		"port": 9200,
		"index": "activedata_requests",
		"type": "request_log",
		"tjson": false,
		"schema": {
			"$ref": "//.../resources/schema/request_log.schema.json"
		}
	},
	"saved_queries": {
		"host": "http://localhost",
		"port": 9200,
		"index": "saved_queries",
		"type": "query",
		"debug": false
	},
	"elasticsearch": {
		"host": "http://localhost",
		"port": 9200,
		"index": "testdata",
		"type": "test_result",
		"debug": false
	},
	"debug": {
		"trace": true
	}
}
