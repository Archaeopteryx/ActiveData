{
	"is_travis": true,
	"flask": {
		"host": "0.0.0.0",
		"port": 5000,
		"debug": false,
		"threaded": true,
		"processes": 1
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		},
 		"pyLibrary.env.big_data.MAX_STRING_SIZE": 100000000,
		"pyLibrary.queries.meta.ENABLE_META_SCAN": true,
		"pyLibrary.queries.expressions.ALLOW_SCRIPTING": true
	},
	"request_logs": {
		"host": "http://localhost",
		"port": 9200,
		"index": "active_data_requests",
		"type": "request_log",
		"schema": {
			"$ref": "//../../resources/schema/request_log.schema.json"
		}
	},
	"saved_queries":{
		"host": "http://localhost",
		"port": 9200,
		"index": "saved_queries",
		"type": "query",
		"debug": false
	},
	"elasticsearch": {
		"host": "http://localhost",
		"port": 9200,
		"index": "testdata",
		"type": "test_result",
		"debug": false
	},
	"debug": {
		"trace": true
	}

}
